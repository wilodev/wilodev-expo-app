{
	"bracketSameLine": true,
	"singleQuote": true,
	"printWidth": 80,
	"tabWidth": 2,
	"useTabs": true,
	"semi": true,
	"trailingComma": "es5",
	"bracketSpacing": true,
	"jsxSingleQuote": false,
	"arrowParens": "always",
	"proseWrap": "never",
	"htmlWhitespaceSensitivity": "strict",
	"endOfLine": "lf"
}
